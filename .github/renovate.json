{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
//    ":dependencyDashboard",
    "helpers:pinGitHubActionDigests"
  ],
  "packageRules": [
    {
      "groupName": "gh minor",
      "matchManagers": ["github-actions"],
      "matchUpdateTypes": ["minor", "patch"]
    }
  ],
  "force": {
    "constraints": {
      "dotnet": "6.0.413"
    }
  }
}
